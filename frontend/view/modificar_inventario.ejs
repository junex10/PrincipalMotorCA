<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("./template/head") %>
        <title>Principal motors CA | Modificar inventario</title>
        <link rel='stylesheet' href='./../static/css/nueva_venta.css'>
        <script>
            window.jQuery = window.$ = require('jquery');
        </script>
</head>

<body>
    <div class="container">
        <div class="login-form">
            <div class="main-div">
                <div class="panel">
                    <h1>Modificar inventario</h1>
                    <br><br>
                </div>
                <form id="producto" enctype="multipart/form-data">

                    <div class="form-group">
                        <h2>Nombre de producto</h2>
                    </div>

                    <div class="form-group">
                        <input type="text" name="nombre_producto" id="nombre_producto" class="form-control"
                            placeholder="Nombre del producto" required>
                    </div>

                    <div class="form-group">
                        <h2>Tipo del producto</h2>
                    </div>

                    <div class="form-group">
                        <select name="tipo_producto" id="tipo_producto" class="form-control" required>
                            <option value="0">Seleccione el tipo de producto</option>
                        </select>
                    </div>

                    <div class="form-group">
                        <h2>Marca</h2>
                    </div>

                    <div class="form-group">
                        <select name="marca" id="marca" class="form-control" required>
                            <option value="0">Seleccione la marca</option>
                        </select>
                    </div>

                    <div class="form-group">
                        <h2>Estado</h2>
                    </div>
                    <div class="form-group">
                        <select name="estado" id="estado" class="form-control" required>
                            <option value="0">Seleccione el estado</option>
                        </select>
                    </div>

                    <button type="button" class="btn btn-primary" id="submit">Modificar</button>

                </form>
            </div>
        </div>
    </div>
    </div>

    <script type='text/javascript' src='./../static/lib/bootstrap/js/bootstrap.bundle.js'></script>
    <script type='text/javascript' src='./../static/js/configuracion/modificar/modificar_inventario.js'></script>
</body>

</html>